<script>
      $(function() {
        $('#about-link').trigger('click');
        setTimeout(function() {
          if($('#myModal').attr('toShow') == "true")
            $('#myModal').modal('show')
        }, 2000);
        $('#myModal').on('hide.bs.modal', function (e) {
        })
        $('#j-capital').change(function(){
            var city = $("#j-province").find("option:selected").attr('va');
            var capital = $("#j-capital").find("option:selected").text();
            if(city == '00' || capital == "请选择") return;
            $("#j-school").empty();
            $.ajax({
              url:'/getSchool',
              type:"GET",
              data:{
                city:city,
                capital:capital
              },
              success:function(param){
                if(param.status == 1){
                  var schools = param.schools;
                  for(var i = 0; i < schools.length; i++){
                    console.log(schools[i])
                    $("<option>"+schools[i]+"</option>").appendTo("#j-school");
                  }
                }
              }
            });
        });
        $(".prompt-close").click(function(){
            window.location = "http://weibo.cctu.com:3000/getRd";
            // $('#myModal').modal('hide');
            // $.ajax({
            //   url:'/getRd',
            //   type:"GET",
            //   data:{
            //     constellation:"",
            //     job:"",
            //     edu:""
            //   }
            // });
        });
        $("#goGetRd").click(function(){
            var xingzuo = $("#j-constellation").find("option:selected").attr('va');
            var job = $("#j-job").find("option:selected").text();
            var school = $("#j-school").find("option:selected").text();
            $('#myModal').modal('hide');
            window.location = "http://weibo.cctu.com:3000/getRd?constellation="+xingzuo+"&job="+job+"&edu="+school;
            // $.ajax({
            //   url:'/getRd',
            //   type:"GET",
            //   data:{
